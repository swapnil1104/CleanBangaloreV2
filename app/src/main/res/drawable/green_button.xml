<?xml version="1.0" encoding="utf-8" ?>

<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item   android:state_pressed="true">
        <shape>
            <gradient android:angle="270" android:endColor="#2C6700" android:startColor="#E5E4D7" />

            <stroke android:width="1dp"  />
            <corners android:radius="5dp" />

        </shape>
    </item>
    <item   android:state_enabled="false">
    <shape>
        <gradient android:angle="270" android:endColor="#9c9c9c" android:startColor="#E5E4D7" />

        <stroke android:width="1dp"  />
        <corners android:radius="5dp" />

    </shape>
</item>
    <item>
        <shape>
            <gradient android:angle="270" android:endColor="#92CD00" android:startColor="#E5E4D7" />

            <corners android:radius="5dp" />
        </shape>
    </item>

</selector>